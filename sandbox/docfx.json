{
  "build": {
    "content": [{
      "files": [
        "**/*.md",
        "**/*.yml"
      ],
      "exclude": [
        "**/obj/**",
        "**/includes/**",
        "README.md",
        "LICENSE",
        "LICENSE-CODE",
        "ThirdPartyNotices"
      ]
    }],
    "resource": [{
      "files": [
        "**/*.png",
        "**/*.jpg",
        "**/*.svg",
        "**/*.gif"
      ],
      "exclude": [
        "**/obj/**",
        "**/includes/**"
      ]
    }],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/sandbox/breadcrumb/toc.json"
    },
    "fileMetaData": {
      "monikerRange": {
        "gamedev/unity/**/*.md": ">= unity-2017.1"
      }
    },
    "markdownEngineName": "markdig",
    "versions": {},
    "template": [],
    "dest": "sandbox"
  }
}